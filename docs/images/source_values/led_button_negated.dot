/* vim: set et sw=4 sts=4: */

digraph {
    graph [rankdir=RL];
    edge [arrowhead=normal, style=solid];

    /* Devices */
    node [shape=rect, style=filled, color="#2980b9", fontname=Arial, fontcolor="#ffffff", fontsize=10];

    LED
    Button

    /* functions */
    node [shape=oval, style=filled, color="#9ec6e0", fontcolor="#ffffff"];

    negated

    Button -> negated -> LED;
}
